  "keywords": [
    "react",
    "react-component",
    "scroll",
    "scroller",
    "scrolls"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^6.4.1",
    "expect": "^1.20.2",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.2",
